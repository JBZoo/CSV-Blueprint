# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true
indent_style = space
indent_size = 4

[*.md]
max_line_length = 150

[Makfile]
indent_style = tab

[{*yml,*yaml}]
indent_size = 2
